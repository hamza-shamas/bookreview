<% provide(:title, @title) %>
<div class="container">
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
      
        <h1><%= @user.user_name %></h1>
        
      </section>
      <section class="stats">
        <%= render 'home/stats' %>
      </section>
    </aside>
    <div class="col-md-8">
      <h3><%= @title %></h3>
      <% if @users.any? %>
        <ul class="usergroup" >
         
          <% @users.each do |user| %>
           <p style="text-align:left; padding:10px;border: 1px solid #E7E9EB"> <%= user.user_name %><p>
          <% end %>
          
        </ul>
      <% end %>
    </div>
  </div>
</div>